APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

# ML (offline) configuration
ML_PYTHON_BIN=python
ML_SCRIPTS_PATH=ml
ML_APPROVAL_MODEL_PATH=ml/models/approval_model.pkl
ML_DEMAND_MODEL_PATH=ml/models/demand_model.pkl

ML_DB_HOST=127.0.0.1
ML_DB_PORT=3306
ML_DB_DATABASE=humanresources
ML_DB_USERNAME=root
ML_DB_PASSWORD=

ML_LEAVE_REQUESTS_TABLE=leave_requests
ML_EMPLOYEES_TABLE=employees

ML_COL_LEAVE_ID=id
ML_COL_LEAVE_USER_ID=user_id
ML_COL_LEAVE_TYPE=leave_type
ML_COL_LEAVE_START_DATE=start_date
ML_COL_LEAVE_END_DATE=end_date
ML_COL_LEAVE_DAYS=days_requested
ML_COL_LEAVE_STATUS=status
ML_COL_LEAVE_CREATED_AT=created_at

ML_COL_EMP_USER_ID=user_id
ML_COL_EMP_DEPARTMENT=department
ML_COL_EMP_POSITION=position
ML_COL_EMP_HIRE_DATE=hire_date

ML_APPROVAL_POSITIVE_STATUSES=approved
