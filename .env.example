ML_APPROVAL_IGNORE_STATUSES=cancelled,test,dummy,null
ML_APPROVAL_PREDICT_STATUSES=pending
ML_RANDOM_STATE=42

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
